<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">

    <!-- Sky gradient -->
    <path
        android:fillType="evenOdd"
        android:pathData="M0,0 L200,0 L200,200 L0,200 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:startColor="#4472C4"
                android:endColor="#6495ED"
                android:type="linear"
                android:angle="270" />
        </aapt:attr>
    </path>

    <!-- Trees -->
    <path
        android:fillColor="#2D5A27"
        android:pathData="M40,180 L60,180 L55,120 L70,120 L50,80 L65,80 L45,40 L35,40 L30,80 L45,80 L40,120 L45,120 L40,180 Z" />

    <path
        android:fillColor="#2D5A27"
        android:pathData="M80,180 L100,180 L95,120 L110,120 L90,80 L105,80 L85,40 L75,40 L70,80 L85,80 L80,120 L85,120 L80,180 Z" />

    <path
        android:fillColor="#2D5A27"
        android:pathData="M120,180 L140,180 L135,120 L150,120 L130,80 L145,80 L125,40 L115,40 L110,80 L125,80 L120,120 L125,120 L120,180 Z" />

    <path
        android:fillColor="#2D5A27"
        android:pathData="M160,180 L180,180 L175,120 L190,120 L170,80 L185,80 L165,40 L155,40 L150,80 L165,80 L160,120 L165,120 L160,180 Z" />

    <!-- Ground -->
    <path
        android:fillColor="#4CAF50"
        android:pathData="M0,180 L200,180 L200,200 L0,200 Z" />
</vector>
